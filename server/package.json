{
  "name": "tinyhouse-server",
  "version": "1.0.0",
  "description": "Back-end application created with Node.js, Express, TypeScript, GraphQL, Apollo and MongoDB",
  "main": "src/index.ts",
  "private": true,
  "scripts": {
    "server": "nodemon -r dotenv/config src/index.ts",
    "build": "tsc -p ./",
    "start": "node build/index.js",
    "lint": "eslint src --ext .ts",
    "seed": "ts-node -r dotenv/config temp/seed.ts",
    "clear": "ts-node -r dotenv/config temp/clear.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/aleksandr-e-lebedev/tinyhouse.git"
  },
  "keywords": [
    "Node.js",
    "Express",
    "TypeScript",
    "GraphQL",
    "Apollo",
    "MongoDB"
  ],
  "author": "Aleksandr Lebedev",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/aleksandr-e-lebedev/tinyhouse/issues"
  },
  "homepage": "https://github.com/aleksandr-e-lebedev/tinyhouse#readme",
  "dependencies": {
    "@googlemaps/google-maps-services-js": "3.1.13",
    "apollo-server-express": "2.18.2",
    "cookie-parser": "1.4.5",
    "express": "4.17.1",
    "googleapis": "65.0.0",
    "graphql": "14.7.0",
    "lodash.merge": "4.6.2",
    "mongodb": "3.6.3"
  },
  "devDependencies": {
    "@tsconfig/node12": "1.0.7",
    "@types/cookie-parser": "1.4.2",
    "@types/express": "4.17.8",
    "@types/lodash.merge": "4.6.6",
    "@types/mongodb": "3.5.33",
    "@types/node": "14.11.10",
    "@typescript-eslint/eslint-plugin": "4.6.0",
    "@typescript-eslint/parser": "4.6.0",
    "dotenv": "8.2.0",
    "eslint": "7.12.1",
    "eslint-config-prettier": "6.14.0",
    "eslint-plugin-node": "11.1.0",
    "nodemon": "2.0.5",
    "prettier": "2.1.2",
    "ts-node": "9.0.0",
    "typescript": "4.0.3"
  },
  "engines": {
    "node": "12.19.0",
    "npm": "6.14.8"
  }
}
